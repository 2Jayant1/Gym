{
  "name": "@fitflex/api",
  "version": "1.0.0",
  "private": true,
  "description": "Gym management system backend",
  "main": "server.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "lint": "eslint . --ext .js --max-warnings 10",
    "test": "jest --forceExit --detectOpenHandles",
    "test:watch": "jest --watch --forceExit",
    "test:coverage": "jest --coverage --forceExit --detectOpenHandles",
    "smoke": "node src/scripts/smoke.js",
    "reseed": "node -e \"fetch('http://127.0.0.1:5001/api/dev/reseed',{method:'POST'}).then(r=>r.text()).then(t=>console.log(t)).catch(e=>{console.error(e);process.exit(1);});\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.10.0",
    "morgan": "^1.10.0",
    "winston": "^3.19.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4"
  }
}
